{
  "manifest_version": 3,
  "name": "Focus 101",
  "description": "Simple extension to block sites and keep you focused!",
  "version": "1.0.1",
  "action": {
    "default_icon": "images/focus_icon.png"
  },
  "options_page": "./home/home.html",
  "background": {
    "service_worker": "worker.js",
    "type": "module"
  },
  "permissions": ["declarativeNetRequest", "storage"],
  "host_permissions": ["<all_urls>"]
}
